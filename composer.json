{
	"name": "vierwd/typo3-base",
	"description": "FORWARD MEDIA TYPO3 base setup",
	"type": "typo3-cms-extension",
	"keywords": [
		"TYPO3",
		"extension"
	],
	"support": {
		"issues": "https://github.com/4wdmedia/vierwd_base/issues"
	},
	"authors": [{
		"name": "FORWARD MEDIA",
		"email": "kontakt@4wdmedia.de",
		"homepage": "https://www.4wdmedia.de",
		"role": "Developer"
	}],
	"require": {
		"php": ">=5.5.0",
		"typo3/cms-core": "^7.6 || ^8.0",
		"league/oauth2-google": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"Vierwd\\VierwdBase\\": "Classes"
		}
	},
	"replace": {
		"vierwd_base": "self.version",
		"vierwd/typo3_base": "self.version"
	},
	"config": {
		"vendor-dir": ".Build/vendor",
		"bin-dir": ".Build/bin",
		"preferred-install": {
			"typo3/cms": "source"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	}
}
